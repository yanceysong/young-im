<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yanceysong.im.domain.group.dao.mapper.ImGroupMapper">
    <select id="getJoinGroupMaxSeq" parameterType="map" resultType="java.lang.Long">
        select max(sequence) from im_group
        where app_id = #{appId} and group_id = #{groupId} in
        <foreach collection="groupId" index="index" item="id"
                 separator="," open="(" close=")"> #{id}
        </foreach>
    </select>
</mapper>